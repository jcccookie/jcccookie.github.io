<div id="head">
      <h1> Class Schedules </h1>
</div>

<form id="class_search" action="" method="">
    <label> Search by your Student ID</label>
    <input id="studentID" type="text" name="studentID" value="">
    <input type="button" value="Search" onclick="getClasses()">
</form>

<table id="studentClasses">
   <thead>
      <th> Term </th>
      <th> Class </th>
      <th> Subject </th>
      <th> Credit </th>
      <th> Fees </th>
      <th> Paid </th>
   </thead>
   <tboad>
      {{#each class}}
      <tr>   
         <td> {{term_name}} </td>
         <td> {{term_year}} </td>
         <td> {{class_name}} </td>
         <td> {{subject_name}} </td>
         <td> {{class_credit}} </td>
         <td>
            <button> Drop </button>
         </td>
      </tr>
   </tbody>
</table>   